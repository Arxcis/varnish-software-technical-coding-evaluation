varnishtest "test vmod-hiring"

server s1 {
	rxreq
	txresp
} -start

varnish v1 -vcl+backend {
        import hiring;

        sub vcl_deliver {
	    set resp.http.Hello = hiring.hello();
        }
} -start

client c1 {
	txreq
	rxresp
	expect resp.status == 200
	expect resp.http.Hello == "vmod-hiring"
} -run
