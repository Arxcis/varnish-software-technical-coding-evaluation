varnishtest "test vmod-playground"

server s1 {
	rxreq
	txresp
} -start

varnish v1 -vcl+backend {
        import playground;

        sub vcl_deliver {
	    set resp.http.Hello = playground.hello();
        }
} -start

client c1 {
	txreq
	rxresp
	expect resp.status == 200
	expect resp.http.Hello == "vmod-playground"
} -run
