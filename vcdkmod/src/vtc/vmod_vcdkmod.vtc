varnishtest "test vmod-vcdkmod"

server s1 {
	rxreq
	txresp
} -start

varnish v1 -vcl+backend {
        import vcdkmod;

        sub vcl_deliver {
	    set resp.http.Hello = vcdkmod.hello();
        }
} -start

client c1 {
	txreq
	rxresp
	expect resp.status == 200
	expect resp.http.Hello == "vmod-vcdkmod"
} -run
